{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive/",
      "files": [
        "dist/jquery.unobtrusive-ajax.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "sweetalert2@10.15.5",
      "destination": "wwwroot/lib/sweetalert2/",
      "files": [
        "dist/sweetalert2.all.min.js",
        "dist/sweetalert2.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-validation@1.19.3",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/jquery.validate.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap@4.6.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    }
  ]
}